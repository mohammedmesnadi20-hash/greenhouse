    {
        "id": "6aea05a0bef3be5d",
        "type": "function",
        "z": "003e8705df617d57",
        "name": "Temp Control (Fan)",
        "func": "let temp = msg.payload.temperature;\nlet fanStatus = context.get('fanStatus') || 0;\nconst ON_THRESHOLD = 30;\nconst OFF_THRESHOLD = 27;\n\nif (temp >= ON_THRESHOLD && fanStatus === 0) fanStatus = 1;\nelse if (temp <= OFF_THRESHOLD && fanStatus === 1) fanStatus = 0;\n\ncontext.set('fanStatus', fanStatus);\nmsg.payload = fanStatus;\nmsg.topic = 'fan';\nreturn msg;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 650,
        "y": 200,
        "wires": [
            [
                "5f02a4b1d76add17"
            ]
        ]
    },
