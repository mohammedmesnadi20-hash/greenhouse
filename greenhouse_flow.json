        "id": "410684a5d839d933",
        "type": "function",
        "z": "003e8705df617d57",
        "name": "Virtual ESP32 Sensors",
        "func": "let temperature = (Math.random() * 10 + 25).toFixed(1); // 25–35°C\nlet humidity = (Math.random() * 20 + 50).toFixed(1);    // 50–70%\nlet soil = (Math.random() * 50 + 30).toFixed(1);         // 30–80%\n\nmsg.payload = {\n    temperature: parseFloat(temperature),\n    humidity: parseFloat(humidity),\n    soil: parseFloat(soil)\n};\nreturn msg;",
        "outputs": 1,
        "x": 360,
        "y": 140,
